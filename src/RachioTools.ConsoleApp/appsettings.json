{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Information",
      "System.Net.Http.HttpClient": "Warning"
    }
  },
  "RachioApi": {
    "BaseUrl": "https://api.rach.io/1/public/",
    "ApiKey": "YOUR_API_KEY"
  },
  "Winterize": {
    "DeviceName": "Rachio Controller",
    "Zones": [
      {
        "ZoneName": "Zone 1",
        "RunTimeSeconds": 300,
        "RestAfterSeconds": 60
      },
      {
        "ZoneName": "Zone 2",
        "RunTimeSeconds": 300,
        "RestAfterSeconds": 60
      }
    ]
  }
}
